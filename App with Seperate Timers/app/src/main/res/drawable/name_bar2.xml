<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <layer-list>
            <item android:width="44dp" android:height="30dp" android:gravity="center_vertical" android:left="122dp" >
                <rotate xmlns:android="http://schemas.android.com/apk/res/android"
                    android:fromDegrees="90"
                    android:toDegrees="90"
                    android:drawable="@drawable/baseline_change_history_24">
                </rotate>
            </item>
            <item android:width="44dp" android:height="30dp" android:gravity="center_vertical">
                <rotate xmlns:android="http://schemas.android.com/apk/res/android"
                    android:fromDegrees="270"
                    android:toDegrees="270"
                    android:drawable="@drawable/baseline_change_history_24">
                </rotate>
            </item>
            <item android:height="40dp" android:width="100dp" android:gravity="center_vertical" android:left="33dp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/grey_dark"></solid>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>